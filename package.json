{
  "name": "danielou-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run postbuild",
    "postbuild": "copyfiles -u 3 \"dist/danielou-portfolio/server/**/*\" netlify/functions/ssr-app && copyfiles -u 3 \"dist/danielou-portfolio/browser/**/*\" netlify/functions/ssr-app/browser && copyfiles -u 3 \"dist/danielou-portfolio/server/polyfills.server.mjs\" netlify/functions && copyfiles -u 3 \"dist/danielou-portfolio/server/polyfills.server.mjs\" netlify/functions/ssr-app && node scripts/patch-server-files.cjs && rm -f netlify/functions/polyfills.server* && rm -f netlify/functions/ssr-app/polyfills.server*",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:danielou-portfolio": "node dist/danielou-portfolio/server/server.mjs",
    "deploy": "ng deploy --base-href=/",
    "dev": "npm run build && netlify dev"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "^19.2.15",
    "@angular/common": "^19.2.15",
    "@angular/compiler": "^19.2.15",
    "@angular/core": "^19.2.15",
    "@angular/forms": "^19.2.15",
    "@angular/platform-browser": "^19.2.15",
    "@angular/platform-browser-dynamic": "^19.2.15",
    "@angular/platform-server": "^19.2.15",
    "@angular/router": "^19.2.15",
    "@angular/ssr": "^19.2.15",
    "@google/generative-ai": "^0.24.1",
    "@netlify/angular-runtime": "^2.4.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "rxjs": "~7.8.0",
    "serverless-http": "^4.0.0",
    "three": "^0.177.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.15",
    "@netlify/functions": "^5.0.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@types/three": "^0.177.0",
    "angular-cli-ghpages": "^2.0.3",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "netlify-cli": "^23.9.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.5.0"
  }
}
